{
  "manifest_version": 3,
  "name": "User Authentication Extension",
  "version": "1.0",
  "description": "A Chrome extension to authenticate users based on a unique ID.",
  "permissions": [
    "storage",
    "identity"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}